{
	"name": "logify",
	"version": "1.0.0",
	"description": "An event logging package that structures and stores the event to its specified location",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build:dist": "rimraf dist && rollup -c",
		"build:docs": "typedoc",
		"build": "npm run build:dist && npm run build:docs",
		"test": "jest",
		"build:doc": "typedoc --out docs src",
		"start": "ts-node src/index.ts"
	},
	"keywords": [
		"typescript",
		"logger",
		"event",
		"logging",
		"structure",
		"storage"
	],
	"author": "Rutvij Sathe",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"eslint": "^8.56.0",
		"jest": "^29.7.0",
		"rimraf": "^5.0.5",
		"rollup": "^4.12.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-prettier": "^4.1.1",
		"rollup-plugin-typescript2": "^0.36.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typedoc": "^0.25.8",
		"typedoc-plugin-markdown": "^3.17.1",
		"typedoc-plugin-missing-exports": "^2.2.0",
		"typescript": "^5.3.3"
	}
}
